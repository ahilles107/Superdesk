<?xml version="1.0" encoding="UTF-8"?>
<project name="test-ally-api" default="build" basedir=".">

	<property name="destination" value="."/>
	<property name="egg.test.ally.api" value="test-ally-api-1.0-py2.6.egg"/>

	<target name="clean">
		<delete file="${destination}/${egg.test.ally.api}" />
	</target>

	<target name="build" depends="clean">
		<zip destfile="${destination}/${egg.test.ally.api}" basedir="../test-ally-api/">
			<exclude name="**/__pycache__/"/>
			<exclude name="/.*"/>
			<exclude name="/*.egg"/>
		</zip>
	</target>

</project>