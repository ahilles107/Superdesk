<?xml version="1.0" encoding="UTF-8"?>
<project name="ally-ioc" default="build" basedir=".">

	<property name="destination" value="."/>
	<property name="egg.ally.ioc" value="ally-ioc-1.0-py3.2.egg"/>

	<target name="clean">
		<delete file="${destination}/${egg.ally.ioc}" />
	</target>

	<target name="build" depends="clean">
		<zip destfile="${destination}/${egg.ally.ioc}" basedir="../src-ally-ioc/">
			<exclude name="**/__pycache__/"/>
			<exclude name="/.*"/>
			<exclude name="/*.egg"/>
		</zip>
	</target>

</project>